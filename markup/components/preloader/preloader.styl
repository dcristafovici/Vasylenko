#preloader
  background #fff
  position fixed
  left 0
  transition all 0.6s ease-in-out
  top 0
  width 100%
  height 100%
  z-index 30
  display flex
  justify-content center
  align-items center
  span
    font-size: 10px;
    color #ffc600
    width: 2em;
    height: 2em;
    margin: 0 0 4em 0;
    background-color: transparent;
    border-radius: 50%;
    animation: spinner-3 2.5s infinite linear;



@keyframes spinner-3 
  0% 
    box-shadow -10.5em 2em transparent, -10.5em 2em transparent, -10.5em 2em transparent, -10.5em 2em transparent
  5% 
    box-shadow -10.5em 2em transparent, -10.5em 2em transparent, -10.5em 2em transparent, -5.5em 2em #ffc600
  10% 
    box-shadow -10.5em 2em transparent, -10.5em 2em transparent, -6.5em 2em #ffc600, -0.5em 2em
  15% 
    box-shadow -10.5em 2em transparent, -7.5em 2em #ffc600, -2.5em 2em, 4.5em 2em
  20% 
    box-shadow -8.5em 2em #ffc600, -4.5em 2em, 1.5em 2em, 4.5em 2em
  25% 
    box-shadow -6.5em 2em, -1.5em 2em, 1.5em 2em, 4.5em 2em
  30%, 55% 
    box-shadow -4.5em 2em, -1.5em 2em, 1.5em 2em, 4.5em 2em
  60% 
    box-shadow -4.5em 2em, -1.5em 2em, 1.5em 2em, 6.5em 2em
  65% 
    box-shadow -4.5em 2em, -1.5em 2em, 4.5em 2em, 8.5em 2em #ffc600
  70% 
    box-shadow -4.5em 2em, 2.5em 2em, 7.5em 2em #ffc600, 10.5em 2em transparent
  75% 
    box-shadow 0.5em 2em, 6.5em 2em #ffc600, 10.5em 2em transparent, 10.5em 2em transparent
  80% 
    box-shadow 5.5em 2em #ffc600, 10.5em 2em transparent, 10.5em 2em transparent, 10.5em 2em transparent
  85%, 100% 
    box-shadow 10.5em 2em transparent, 10.5em 2em transparent, 10.5em 2em transparent, 10.5em 2em transparent



body.pace-done
  #preloader
    opacity 0
    visibility: hidden
    z-index -1